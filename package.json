{
  "name": "react-portal-app",
  "version": "0.1.0",
  "private": true,
  "homepage": "http://vijay-kumar-singh.github.io/react-portal-app",
  "dependencies": {
    "@material-ui/core": "^4.9.10",
    "@material-ui/styles": "^4.9.10",
    "@types/enzyme": "^3.10.5",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/node": "12.0.10",
    "@types/react": "16.8.22",
    "@types/react-dom": "16.8.4",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.4.4",
    "node-sass": "^4.12.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-icons": "^3.7.0",
    "react-router-dom": "^5.0.1",
    "react-scripts": "5.0.1",
    "styled-components": "^4.3.2"
  },
  "scripts": {
    "build:css": "node-sass src/assets/styles/sass/ -o src/assets/styles/css/",
    "watch:css": "npm run build:css && node-sass src/assets/styles/sass/ -o src/assets/styles/css/ --watch --recursive",
    "start:js": "react-scripts start",
    "start": "npm-run-all -p watch:css start:js",
    "build": "npm run build:css && react-scripts build",
    "test": "react-scripts test",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "eject": "react-scripts eject"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/jest": "^24.0.15",
    "@types/node-sass": "^4.11.0",
    "@types/prop-types": "^15.7.3",
    "@types/react-icons": "^3.0.0",
    "@types/react-router-dom": "^4.3.4",
    "@types/styled-components": "^4.1.16",
    "@typescript-eslint/eslint-plugin": "^2.27.0",
    "@typescript-eslint/parser": "^2.27.0",
    "eslint": "^5.16.0",
    "eslint-plugin-react": "^7.19.0",
    "gh-pages": "^2.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.4",
    "source-map-loader": "^0.2.4",
    "ts-loader": "^6.2.2",
    "typescript": "^3.5.2"
  }
}
